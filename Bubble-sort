#include <stdio.h>
#include <conio.h>
void main()
{
    int i,n=0,j;
    printf("enter size of array:");
    scanf("%d",&n);
    int a[n];
    printf("enter array elemants:");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
        }
        for(i=0;i<n;i++){
            for(j=0;j<n-1;j++){
                if(a[j]>a[j+1]){
                int t=a[j];a[j]=a[j+1];a[j+1]=t;
                }
            }
        }
        for(i=0;i<n;i++){
            printf("%d ",a[i]);
        }

    
}
